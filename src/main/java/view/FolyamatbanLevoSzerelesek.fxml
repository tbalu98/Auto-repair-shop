<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FolyamatbanLevoSzerelesekController">
    <children>
        <VBox layoutX="100.0" layoutY="72.0" prefHeight="400.0" prefWidth="400.0" spacing="10.0">
            <children>
                <TableView fx:id="FolyamatbanLevoSzerelesekTablaNezet" prefHeight="300.0" prefWidth="400.0">
                    <columns>
                        <TableColumn fx:id="RendszamOszlop" minWidth="0.0" prefWidth="200.0" text="Rendszám" />
                        <TableColumn fx:id="MunkavegzesKezdeteOszlop" prefWidth="200.0" text="Munkavégzés kezdete" />
                    </columns>
                </TableView>
                <HBox prefHeight="40.0" prefWidth="600.0" spacing="10.0">
                    <children>
                        <TextField fx:id="MunkavegzesKoltsege" promptText="Munkavégzés költsége" />
                        <Button mnemonicParsing="false" onAction="#szerelesVegePushed" text="Szerelés vége" />
                    </children>
                </HBox>
            <Button mnemonicParsing="false" onAction="#visszaazUjSzerelesekFelvetelehezPushed" text="Vissza az új szereles felvételéhez" />
            </children>
        </VBox>
    </children>
</AnchorPane>
